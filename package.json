{
  "name": "@dolomite-exchange/dolomite-margin-modules",
  "version": "0.0.8",
  "description": "Repository uses the DolomiteMargin instance on Arbitrum One for testing new modules and features against the live deployment.",
  "directories": {
    "test": "test"
  },
  "files": [
    "scripts/deployments.json"
  ],
  "scripts": {
    "build": "hardhat compile",
    "clean": "hardhat clean",
    "coverage": "yarn coverage:prepare && COVERAGE=true node --max-old-space-size=32768 ./node_modules/.bin/hardhat coverage --testfiles \"test/**/!(*Liquidation*|*LiquidatorProxyV2*|*Umami*)*.ts\" --network hardhat",
    "coverage:prepare": "ts-node scripts/fix-contracts-for-coverage.ts",
    "coverage:specific": "yarn coverage:prepare && COVERAGE=true node --max-old-space-size=32768 ./node_modules/.bin/hardhat coverage --testfiles \"test/external/**/(GmxV2*|IsolationModeFreezableLiquidatorProxy)*.ts\" --network hardhat",
    "coverage:specific2": "yarn coverage:prepare && COVERAGE=true node --max-old-space-size=32768 ./node_modules/.bin/hardhat coverage --testfiles \"test/external/gmxV2/GmxV2IsolationModeWrapperTraderV2.ts\" --network hardhat",
    "deploy:abracadabra-ecosystem": "hardhat --network arbitrum_one run scripts/deploy/deploy-abracadabra-ecosystem.ts",
    "deploy:atlas-ecosystem": "hardhat --network arbitrum_goerli run scripts/deploy/deploy-atlas-ecosystem.ts",
    "deploy:coveralls": "coveralls < coverage/lcov.info",
    "deploy:chainlink-automation-price-oracles": "hardhat --network arbitrum_one run scripts/deploy/deploy-chainlink-automation-price-oracles.ts",
    "deploy:deprecate-trader-v1": "hardhat --network arbitrum_one run scripts/deploy/zap/deploy-deprecate-trader-v1.ts",
    "deploy:dolomite-registry": "hardhat --network arbitrum_one run scripts/deploy/deploy-dolomite-registry.ts",
    "deploy:gmx-ecosystem": "hardhat --network arbitrum_one run scripts/deploy/deploy-gmx-ecosystem.ts",
    "deploy:stablecoin-interest-setter": "hardhat --network arbitrum_one run scripts/deploy/interest-setters/deploy-stablecoin-interest-setter.ts",
    "deploy:jones-usdc-ecosystem": "hardhat --network arbitrum_one run scripts/deploy/deploy-jones-usdc-ecosystem.ts",
    "deploy:odos-ecosystem": "hardhat --network arbitrum_one run scripts/deploy/traders/deploy-odos-ecosystem.ts",
    "deploy:paraswap-ecosystem": "hardhat --network arbitrum_one run scripts/deploy/traders/deploy-paraswap-ecosystem.ts",
    "deploy:pendle-pt-glp-ecosystem": "hardhat --network arbitrum_one run scripts/deploy/pendle/deploy-pendle-pt-glp-ecosystem.ts",
    "deploy:pendle-yt-glp-ecosystem": "hardhat --network arbitrum_one run scripts/deploy/pendle/deploy-pendle-yt-glp-ecosystem.ts",
    "deploy:plutus-ecosystem": "hardhat --network arbitrum_one run scripts/deploy/deploy-plutus-ecosystem.ts",
    "deploy:upgrade-trader-v2": "hardhat --network arbitrum_one run scripts/deploy/zap/deploy-upgrade-trader-v2.ts",
    "deploy:zap-part-2": "hardhat --network arbitrum_one run scripts/deploy/zap/deploy-zap-part-2.ts",
    "deploy:zap-part-3": "hardhat --network arbitrum_one run scripts/deploy/zap/deploy-zap-part-3.ts",
    "deploy:zap-part-4": "hardhat --network arbitrum_one run scripts/deploy/zap/deploy-zap-part-4.ts",
    "execute:arbitrum-105": "hardhat --network arbitrum_one run scripts/deploy/safe-transactions/arbitrum/safe-transaction-105.ts",
    "execute:arbitrum-106": "hardhat --network arbitrum_one run scripts/deploy/safe-transactions/arbitrum/safe-transaction-106.ts",
    "execute:arbitrum-108": "hardhat --network arbitrum_one run scripts/deploy/safe-transactions/arbitrum/safe-transaction-108.ts",
    "lint": "yarn lint:sol && yarn lint:ts",
    "lint:sol": "hardhat check && python scripts/lint-contracts.py fix",
    "lint:ts": "tslint --project . -t verbose 'src/**/*.ts' 'test/**/*.ts'",
    "sort:contracts": "hardhat --network hardhat run scripts/sort-contract.ts",
    "test": "hardhat --network hardhat test",
    "test:specific": "TEST_SPECIFIC=true hardhat --max-memory 32768 --network hardhat test test/external/liquidators/IsolationModeFreezableLiquidatorProxy.ts",
    "test:specific2": "TEST_SPECIFIC=true hardhat --max-memory 32768 --network hardhat test test/external/gmxV2/GmxV2IsolationModeUnwrapperTraderV2.ts",
    "test:specific3": "TEST_SPECIFIC=true hardhat --max-memory 32768 --network hardhat test test/external/gmxV2/GmxV2IsolationModeWrapperTraderV2.ts",
    "test:specific4": "TEST_SPECIFIC=true hardhat --max-memory 32768 --network hardhat test test/external/gmxV2/GmxV2IsolationModeTokenVaultV1.ts",
    "test:specific5": "TEST_SPECIFIC=true hardhat --max-memory 32768 --network hardhat test test/external/gmxV2/*.ts",
    "verify:contract": "hardhat --network arbitrum_one run scripts/verify-contract.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dolomite-exchange/dolomite-margin-modules.git"
  },
  "author": "",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/dolomite-exchange/dolomite-margin-modules/issues"
  },
  "homepage": "https://github.com/dolomite-exchange/dolomite-margin-modules#readme",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@chainlink/contracts": "^0.6.1",
    "@dolomite-exchange/dolomite-margin": "^0.9.29",
    "@dolomite-exchange/zap-sdk": "^0.1.24",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-solhint": "^3.0.1",
    "@nomiclabs/hardhat-vyper": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "4.9.3",
    "@openzeppelin/contracts-ethereum-package": "^3.0.0",
    "@openzeppelin/upgrades": "^2.8.0",
    "@pendle/sdk-v2": "^2.6.0",
    "@typechain/ethers-v5": "^11.1.1",
    "@typechain/hardhat": "^8.0.3",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "axios": "^1.2.1",
    "chai": "^4.3.6",
    "chai-bignumber": "^3.0.0",
    "coveralls": "^3.1.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.16.0",
    "ethereum-waffle": "^4.0.0-alpha.0",
    "ethers": "^5.6.0",
    "hardhat": "^2.16.1",
    "hardhat-gas-reporter": "^1.0.9",
    "hardhat-tracer": "^2.6.0",
    "solidity-coverage": "^0.8.5",
    "ts-node": "^10.5.0",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {}
}
